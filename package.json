    "build": "webpack -p --config scripts/webpack.config.js",
    "build_umd": "webpack -p --config scripts/webpack.config.umd.js"
  },
  "dependencies": {
    "mithril": "1.1.3",
    "mithril-stream": "^1.1.0",
    "polythene-fastclick": "^1.0.0-rc.4",
